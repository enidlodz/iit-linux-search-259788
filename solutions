grep "/login" /workspaces/iit-linux-search-259788/logs/access_small.log | wc -l
grep -o "Smith" /workspaces/iit-linux-search-259788/names/fullnames_with_age.txt | wc -l
grep -o "Smith" /workspaces/iit-linux-search-259788/names/fullnames_simple.txt [FILE DOES NOT EXIST]
grep -oP "age=\K.*" /workspaces/iit-linux-search-259788/names/fullnames_with_age.txt | sort | uniq -c | sort -nr | head -n 1
grep -oP "^[A-Z][a-zA-Z]+\s+[A-Z][a-zA-Z]+" /workspaces/iit-linux-search-259788/names/fullnames_with_age.txt | sort | uniq -c | sort -nr | head -n 10
grep -oP "user=\K[^ ]+" /workspaces/iit-linux-search-259788/logs/app_small.log | sort | uniq | wc -l
grep -oP "action=\K[^ ]+" /workspaces/iit-linux-search-259788/logs/app_medium.log | sort | uniq -c | sort -nr
grep -oP "^\S+\s+\S+\s+\K\S+" /workspaces/iit-linux-search-259788/logs/app_small.log | sort | uniq -c | sort -nr
grep -oP "task=\K[^ ]+" /workspaces/iit-linux-search-259788/logs/system_small.log | sort | uniq -c | sort -nr | head -n 1

\K - meta reset
\S - non-whitespace
\s - whitespace
+ - greedy
^ - position at start of line (assertion), alternatively when in brackets;
[^] - literally anything except regex match given after ^ in brackets, ie. [^ ] returns anything besides " "
[A-Z] - any capital
[a-zA-Z] - any letter
-o = occurence
-P = Perl-compatbile Regex Expression flag
wc -l = count lines
sort = sort
sort -nr = sort numerically by first value, reverse (inc -> dec)
uniq = return only unique occurences, works only with sort
uniq -c = count occurences, return count and unique word
head -n VALUE = grab only VALUE amount of lines from the top (head) of returned text